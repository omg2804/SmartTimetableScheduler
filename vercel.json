{
  "version": 2,
  "buildCommand": "cd newer && npm run build",
  "outputDirectory": "newer/build",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/embed",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/generate.js": {
      "maxDuration": 30
    }
  }
}
